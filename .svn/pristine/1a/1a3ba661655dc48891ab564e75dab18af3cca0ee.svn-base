.easyui-accordion ul {
	list-style-type: none;
	margin: 0px;
	padding: 10px;
}

.easyui-accordion ul li {
	padding: 0px;
}

.easyui-accordion ul li a {
	line-height: 24px;
	color:#8D8E8E;
}

.easyui-accordion ul li div {
	margin: 5px 0px;
	padding-left: 10px;
	padding-top: 2px;
   	border: 1px solid #0369BE;
    background:#fff;
}

.easyui-accordion ul li div.hover {
	border:1px solid #46A2EF;
	background: #46A2EF;
	cursor: pointer;
	border-radius: 1px; /* 所有角都使用半径为5px的圆角，此属性为CSS3标准属性 */ 
-moz-border-radius:1px; /* Mozilla浏览器的私有属性 */ 
-webkit-border-radius: 1px; /* Webkit浏览器的私有属性 */ 
border-radius: 1px; /* 四个半径值分别是左上角、右上角、右下角和左下角 */ 
}

.easyui-accordion ul li div.hover a {
	color: #fff;
}

.easyui-accordion ul li div.selected {
	/*border:1px solid #B5B5B5;
	background:#CFD0CF;*/
	    border: 1px solid #399ED2;
    background: #51ACFD;
	cursor: default;
	color:#fff;
	
		border-radius: 1px; /* 所有角都使用半径为5px的圆角，此属性为CSS3标准属性 */ 
-moz-border-radius:1px; /* Mozilla浏览器的私有属性 */ 
-webkit-border-radius: 1px; /* Webkit浏览器的私有属性 */ 
border-radius: 1px; /* 四个半径值分别是左上角、右上角、右下角和左下角 */ 
}

.easyui-accordion ul li div.selected a {
	color: #fff;

}

